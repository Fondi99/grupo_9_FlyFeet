<header>
  <nav>
    <div class="navbar-toggler">
      <a href=""><i class="icon fas fa-bars fa-2xl"></i></a>
    </div>
    <div class="logo">
      <a href="/">
        <img src="/images/Logo.png" alt="Logo" />
        <span class="fly">FLY</span><span class="feet">FEET</span>
      </a>
    </div>
    <a href="/list"><div class="products">PRODUCTOS</div></a>
    <div class="user">
      <% if(locals.user){ %>
      <a href="/users/profile" class="login">
        <img src="/images/user.png" alt="" />
        <span><%= user.first_name %></span>
      </a>
      <% } else { %>
      <a href="/users/login" class="login">
        <img src="/images/user.png" alt="" />
        <span>Log in</span>
      </a>
      <% } %>
      <div class="cart">
        <a href="/cart" class="cart">
          <img src="/images/Vector.png" alt="" />
        </a>
      </div>
    </div>
  </nav>
</header>
